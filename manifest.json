{
  "manifest_version": 3,
  "name": "Chrome ToDo",
  "description": " Simple and clean to-do list extension!",
  "version": "1.0",
  "action": {
    "default_popup": "index.html"
  },
  "permissions": ["storage"],
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "128": "icons/128.png"
  }
}
